    <template title="ls_seoclerks_signature" version_id="1" version_string="1.0.0"><![CDATA[{$message}
<div class="baseHtml signature ugc">
<aside id="seoclerks_ad_{$adLink}">
</aside>
<script type="application/javascript">
!function($, window, document, _undefined)
{
XenForo.Ad{$adLink} = function($ad)
{
var url = '{xen:raw $url}';
if(url != '')
{
$.get(url, function(data) {
  $('#seoclerks_ad_{$adLink}').html(data);
});
}
};
    XenForo.register('#seoclerks_ad_{$adLink}', 'XenForo.Ad{$adLink}');
}
(jQuery, this, document);
</script>
</div>]]></template>
